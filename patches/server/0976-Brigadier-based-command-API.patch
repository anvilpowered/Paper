+package io.papermc.paper.command.brigadier;
+
+import com.mojang.brigadier.CommandDispatcher;
+import com.mojang.brigadier.builder.ArgumentBuilder;
+import com.mojang.brigadier.tree.CommandNode;
+import com.mojang.brigadier.tree.LiteralCommandNode;
+import org.bukkit.Bukkit;
+import org.bukkit.plugin.Plugin;
+import org.jetbrains.annotations.ApiStatus;
+import org.jetbrains.annotations.NotNull;
+import org.jetbrains.annotations.Nullable;
+
+import java.util.ArrayList;
+import java.util.Arrays;
+import java.util.List;
+
+public class CommandBuilderImpl extends CommandBuilder {
+
+    public static class ProviderImpl implements CommandBuilderProvider {
+
+        @Override
+        public CommandBuilder create(Plugin plugin, String name) {
+            return new CommandBuilderImpl(plugin, name);
+        }
+    }
+
+    private final Plugin plugin;
+    private final String literal;
+
+    private String description;
+    private List<String> aliases = List.of();
+
+    protected CommandBuilderImpl(@NotNull Plugin plugin, @NotNull String literal) {
+        this.literal = literal;
+        this.plugin = plugin;
+    }
+
+    @Override
+    public @NotNull CommandBuilder aliases(@NotNull String... aliases) {
+        this.aliases = Arrays.asList(aliases);
+        return this.getThis();
+    }
+
+    @Nullable
+    @Override
+    public String getDescription() {
+        return this.description;
+    }
+
+    @Override
+    public @NotNull CommandBuilder description(@Nullable String description) {
+        this.description = description;
+        return this.getThis();
+    }
+
+    @NotNull
+    @Override
+    public Plugin getPlugin() {
+        return this.plugin;
+    }
+
+    @NotNull
+    @Override
+    protected CommandBuilderImpl getThis() {
+        return this;
+    }
+
+    @NotNull
+    @Override
+    public CommandNode<CommandSourceStack> build() {
+        throw new UnsupportedOperationException();
+    }
+
+    private PluginCommandNode buildWithLiteral(String literal) {
+        return new PluginCommandNode(literal, this);
+    }
+
+
+    @Override
+    public boolean register() {
+        return this.register(Bukkit.getCommandDispatcher());
+    }
+
+    public boolean register(@NotNull CommandDispatcher<CommandSourceStack> dispatcher) {
+        String identifier = this.plugin.getPluginMeta().getName();
+        boolean registeredNode = false;
+        PluginCommandNode root = this.buildWithLiteral(identifier + ":" + this.literal);  // Treat the keyed version of the command as the root
+
+        registeredNode |= this.registerIntoDispatcher(dispatcher, root);
+        registeredNode |= this.registerRedirect(dispatcher, root, this.literal);
+
+        // Add aliases
+        List<String> registeredAliases = new ArrayList<>();
+        for (String alias : this.aliases) {
+            if (registeredNode |= this.registerRedirect(dispatcher, root, alias)) {
+                registeredAliases.add(alias);
+            }
+            if (registeredNode |= this.registerRedirect(dispatcher, root, identifier + ":" + alias)) {
+                registeredAliases.add(identifier + ":" + alias);
+            }
+        }
+
+        root.setAliases(registeredAliases);
+
+        return registeredNode;
+    }
+
+    @SuppressWarnings("deprecation")
+    private boolean registerRedirect(@NotNull CommandDispatcher<CommandSourceStack> dispatcherRoot, @NotNull LiteralCommandNode<CommandSourceStack> node, @NotNull String name) {
+        CommandBuilderImpl commandBuilder = (CommandBuilderImpl) CommandBuilderImpl.newCommandBuilder(this.plugin, name)
+            .description(this.description)
+            .redirect(node)
+            .executes(node.getCommand());
+
+       return this.registerIntoDispatcher(dispatcherRoot, commandBuilder.buildWithLiteral(this.literal));
+    }
+
+    private boolean registerIntoDispatcher(CommandDispatcher<CommandSourceStack> stack, LiteralCommandNode<CommandSourceStack> node) {
+        boolean hasChild = stack.getRoot().getChild(node.getLiteral()) != null;
+        // Avoid merging behavior, we don't want it here.
+        if (!hasChild) {
+            stack.getRoot().addChild(node);
+            return true;
+        }
+
+        return false;
+    }
+}
